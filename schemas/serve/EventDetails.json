{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"properties": {
		"EventDetails": {
			"$ref": "#/definitions/EventDetails"
		}
	},
	"required": ["EventDetails"],
	"title": "EventDetails",
	"definitions": {
		"EventDetails": {
			"$id": "#/properties/EventDetails",
			"type": "object",
			"title": "Event Details schema",
			"required": ["needDeliverableId","eventType"],
			"properties": {
				"needDeliverableId": { "type": "string" },
				"eventType": { "type": "string" },
				"details": {
          					"type": "string"
        					},
				"eventDate": {
          					"type": "string",
          					"format": "date-time"
        					},									
				"status": { 
                                                                     "type": "string" ,
                                                                      "enum": ["Not Started",
                                                                                     "Scheduled",
                                                                                     "In Progress",
                                                                                     "Completed",
                                                                                     "Cancelled"]},
				"comments": { "type": "string" }
			}
		}
	},
	"_osConfig": {
		"systemFields": [
     			 "osCreatedAt",
      			"osUpdatedAt",
      			"osCreatedBy",
     			 "osUpdatedBy"
    		]
		
	}
}
