{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"properties": {
		"Sessions": {
			"$ref": "#/definitions/SessionDetails"
		}
	},
	"required": ["SessionDetails"],
	"title": "SessionDetails",
	"definitions": {
		"SessionDetails": {
			"$id": "#/properties/SessionDetails",
			"type": "object",
			"title": "Sessions schema",
			"required": ["needDeliverableId","startTime", "endTime"],
			"properties": {
				"needDeliverableId": { "type": "string" },
				"startTime": {
          					"type": "string",
          					"format": "date-time"
        					},
				"endTime": {
          					"type": "string",
          					"format": "date-time"
        					},
				"sessionDate": {
          					"type": "string",
          					"format": "date-time"
        					},									
				"status": { 
                                                                     "type": "string" ,
                                                                      "enum": ["Not Started",
                                                                                     "Scheduled",
                                                                                     "In Progress",
                                                                                     "Completed",
                                                                                     "Cancelled"]},
				"comments": { "type": "string" }
			}
		}
	},
	"_osConfig": {
		"systemFields": [
     			 "osCreatedAt",
      			"osUpdatedAt",
      			"osCreatedBy",
     			 "osUpdatedBy"
    		]
		
	}
}
