{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "Entity": {
      "$ref": "#/definitions/Entity"
    }
  },
  "required": [
    "Entity"
  ],
  "title": "Entity",
  "definitions": {
    "Entity": {
      "$id": "#/properties/Entity",
      "type": "object",
      "title": "Entity schema",
      "required": [
        "name",
        "contactDetails",
        "category"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "contactDetails": {
          "$ref": "#/definitions/ContactDetails"
        },
        "website": {
          "$id": "#/properties/website",
          "type": "string",
          "format": "uri",
          "title": "Website"
        },
        "category": {
          "$id": "#/properties/category",
          "type": "string"
        }
      }
    },
    "ContactDetails": {
      "$id": "#/definitions/ContactDetails",
      "type": "object",
      "title": "Contact Details",
      "description": "Contact Details",
      "required": [],
      "properties": {
        "email": {
          "type": "string",
          "title": "Email"
        },
        "mobile": {
          "type": "string",
          "title": "Mobile"
        },
        "address": {
          "$ref": "#/definitions/Address",
          "title": "Address"
        }
      }
    },
    "Address": {
      "type": "object",
      "required": [],
      "additionalProperties": false,
      "properties": {
        "plot": {
          "type": "string",
          "title": "Plot"
        },
        "street": {
          "type": "string",
          "title": "Street"
        },
        "landmark": {
          "type": "string",
          "title": "Landmark"
        },
        "locality": {
          "type": "string",
          "title": "Locality"
        },
        "state": {
          "type": "string",
          "title": "State",
          "enum": [
            "Andaman and Nicobar Islands",
            "Andhra Pradesh",
            "Arunachal Pradesh",
            "Assam",
            "Bihar",
            "Chandigarh",
            "Chhattisgarh",
            "Dadra and Nagar Haveli",
            "Daman and Diu",
            "Delhi",
            "Goa",
            "Gujarat",
            "Haryana",
            "Himachal Pradesh",
            "Jammu and Kashmir",
            "Jharkhand",
            "Karnataka",
            "Kerala",
            "Ladakh",
            "Lakshadweep",
            "Madhya Pradesh",
            "Maharashtra",
            "Manipur",
            "Meghalaya",
            "Mizoram",
            "Nagaland",
            "Odisha",
            "Puducherry",
            "Punjab",
            "Rajasthan",
            "Sikkim",
            "Tamil Nadu",
            "Telangana",
            "Tripura",
            "Uttar Pradesh",
            "Uttarakhand",
            "West Bengal"
          ]
        },
        "district": {
          "type": "string",
          "title": "District"
        },
        "village": {
          "type": "string",
          "title": "Village/Town/City"
        },
        "pincode": {
          "type": "string",
          "title": "Pincode"
        }
      }
    }
  },
  "_osConfig": {
    "ownershipAttributes": [
      {
        "email": "/email",
        "mobile": "/phoneNumber"
      }
    ],
    "privateFields": [
      "$.contactDetails.email",
      "$.contactDetails.mobile",
      "$.contactDetails.address.plot",
      "$.contactDetails.address.street",
      "$.contactDetails.address.landmark",
      "$.contactDetails.address.locality"
    ],
    "indexFields": [],
    "uniqueIndexFields": [
      "name"
    ],
    "systemFields": [
      "osCreatedAt",
      "osUpdatedAt",
      "osCreatedBy",
      "osUpdatedBy"
    ],
    "inviteRoles": [
      "anonymous"
    ]
  }
}
